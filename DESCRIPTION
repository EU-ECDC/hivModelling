Package: hivModelling
Type: Package
Title: Evidence-Based Methods to Calculate HIV Incidence in a Given Population
Version: 0.3.0
Date: 2019-05-16
Authors@R: c(person("Magdalena", "Rosinska", email = "mrosinska@pzh.gov.pl", role = c("aut", "cph")), 
    person("Ard", "van Sighem", email = "a.i.vansighem@amc.uva.nl", role = c("aut")), 
    person("Nikos", "Pantazis", email = "npantaz@med.uoa.gr", role = c("aut")), 
    person("Chantal", "Quinten", email = "chantal.quinten@ecdc.europa.eu", role = c("aut", "ctb")),
